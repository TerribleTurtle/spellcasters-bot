{
  "name": "spellcasters-bot",
  "version": "2.1.0",
  "description": "Discord bot for Spellcasters Community API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "pack": "powershell -Command \"if (Test-Path spellcasters-bot-v2.zip) { Remove-Item spellcasters-bot-v2.zip }; Compress-Archive -Path dist,src,package.json,package-lock.json,tsconfig.json,build_api.py,DEPLOYMENT.md,deploy.sh,.env.example,.prettierrc,eslint.config.mjs,scripts,data,README.md,LICENSE -DestinationPath spellcasters-bot-v2.zip\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "validate": "python build_api.py",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "discord",
    "bot",
    "spellcasters",
    "community",
    "api"
  ],
  "author": "TerribleTurtle",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "fuse.js": "^7.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "undici": "^6.23.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
